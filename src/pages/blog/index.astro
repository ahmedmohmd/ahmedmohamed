---
import BaseLayout from "../../layouts/BaseLayout.astro";
const articles = await Astro.glob("../../data/articles/*.mdx");
---

<BaseLayout title="Welcome to Astro.">
  <h1>Welcome to Astro: Titles</h1>

  <div>
    {articles.map((article: any) => <span>{article.frontmatter.title}</span>)}
  </div>
</BaseLayout>
